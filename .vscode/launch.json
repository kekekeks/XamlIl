{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "BenchmarksCompiler",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/BenchmarksCompiler/bin/Debug/netcoreapp2.2/BenchmarksCompiler.dll",
            "args": [
                "${workspaceFolder}/src/Benchmarks/obj/Debug/netcoreapp2.2/Benchmarks.dll"
            ],
            "cwd": "${workspaceFolder}/src/BenchmarksCompiler",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}